{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "drs-parser": {
      "projectType": "library",
      "root": "projects/drs-parser",
      "sourceRoot": "projects/drs-parser/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/drs-parser/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/drs-parser/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/drs-parser/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "tsConfig": "projects/drs-parser/tsconfig.spec.json",
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ]
          }
        }
      }
    }
  },
  "cli": {
    "analytics": "328460a8-e39a-4c72-8a9b-4674f97a9a5f"
  }
}
